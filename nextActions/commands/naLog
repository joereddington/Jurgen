diff  ~/Jurgen/nextActions/logs/nextActions.skin  ~/Jurgen/nextActions/logs/lastActions.skin | grep "<\\|>" | sort >> ~/Jurgen/nextActions/logs/changes_to_next_Actions_bare.txt
cp  ~/Jurgen/nextActions/logs/nextActions.skin  ~/Jurgen/nextActions/logs/lastActions.skin
