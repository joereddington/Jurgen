date >> ~/Jurgen/nextActions/logs/changes_to_next_Actions.txt
diff  ~/Jurgen/nextActions/nextActions.csv  ~/Jurgen/nextActions/logs/lastActions.csv | grep "<\\|>" >> ~/Jurgen/nextActions/logs/changes_to_next_Actions.txt
cp  ~/Jurgen/nextActions/nextActions.csv  ~/Jurgen/nextActions/logs/lastActions.csv
